    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "test": "echo \"No tests configured\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/ --cache --cache-location ../../../work/eslint/.eslintcache",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix --cache --cache-location ../../../work/eslint/.eslintcache",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md}\" --cache --cache-location ../../../work/prettier/.prettiercache",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,json,md}\" --cache --cache-location ../../../work/prettier/.prettiercache"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
